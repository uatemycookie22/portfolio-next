{
  "name": "portfolio-2022",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"tailwindcss --input ./styles/input.css --output ./public/build/tailwind.css --watch\" \"next dev --turbopack\"",
    "build": "concurrently \"postcss styles/input.css -o public/build/tailwind.css\" \"next build\"",
    "start": "next start --keepAliveTimeout 70000",
    "lint": "next lint",
    "build:css": "postcss styles/input.css -o public/build/tailwind.css",
    "dev:css": "tailwindcss --input ./styles/input.css --output ./public/build/tailwind.css --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@next/bundle-analyzer": "^15.5.6",
    "@tanstack/react-query": "^5.90.6",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.13",
    "eslint": "8.22.0",
    "eslint-config-next": "^15.5.6",
    "jotai": "^1.13.1",
    "next": "^15.5.6",
    "pocketbase": "^0.14.4",
    "postcss-cli": "^10.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "sass": "^1.56.2",
    "sharp": "^0.32.1",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
